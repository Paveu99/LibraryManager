{
  "name": "library-manager-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint-staged": "eslint --report-unused-disable-directives --max-warnings 100",
