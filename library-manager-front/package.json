    "@mui/x-charts": "^7.24.1",
    "@mui/x-date-pickers": "^7.24.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.63.0",
    "@tanstack/react-router": "^1.95.1",
    "@tanstack/router-plugin": "^1.95.1",
    "@types/express": "^5.0.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
